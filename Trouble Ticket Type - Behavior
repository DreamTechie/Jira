    		VRF.setFormValue(null)
    		POD.setFormValue(null)
        	Destination_POD.setFormValue(null)
    		Destination_Port.setFormValue(null)
        
		//Set Netscout Values to null if no issue ID is populated (ie, a new record)
        	SPECIFIC_TO_A_SINGLE_ISSUE_CASE.setFormValue(null)
    		EASILY_REPRODUCED.setFormValue(null)
    		OCCURS_WHEN_EXECUTED_MANUALLY.setFormValue(null)
    		FIRST_TIME_ISSUE_OCCURRED.setFormValue(null)
    		ISSUE_START_DATE_AND_TIME.setFormValue(null)
    		ISSUE_END_DATE_AND_TIME.setFormValue(null)
    		FREQUENCY_OF_OCCURRENCE.setFormValue(null)*/
     }
    
     else{}
    
    
}
